<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle" >
   <!--背景颜色-->
   <solid android:color="@color/white" />

   <!--设置边距-->
   <padding
       android:bottom="1dp"
       android:left="1dp"
       android:right="1dp"
       android:top="1dp" />

   <!--控制边界线颜色和大小-->
   <stroke
       android:width="1dp"
       android:color="@color/unline" />
</shape>  
